e":2,"action":[{"type":"shake","y":30,"delay":0,"dur":0.15,"number":2,"x":0}],"painting":{"time":1,"alpha":0.3},"bgName":"star_level_bg_100","actor":205021},{"expression":0,"nameColor":"#a9f548","say":"That's right! The Royal Maids will be stunned speechless by our magnificent Christmas present!","dir":1,"hidePaintObj":true,"side":2,"painting":{"time":1,"alpha":0.3},"typewriter":{"speedUp":0.01,"speed":0.05},"bgName":"star_level_bg_100","actor":299011},{"typewriter":{"speedUp":0.01,"speed":0.05},"side":2,"painting":{"time":1,"alpha":0.3},"say":"A-anyway, really appreciate the tips! Please swing by the Christmas party later and have some fun!","actor":199012,"dir":1,"bgName":"star_level_bg_100","nameColor":"#a9f548"},{"typewriter":{"speedUp":0.01,"speed":0.05},"side":2,"painting":{"time":1,"alpha":0.3},"say":"Now if you'll excuse me, I've gotta go. The gift exchange should be starting soon.","actor":199012,"dir":1,"bgName":"star_level_bg_100","nameColor":"#a9f548"},{"typewriter":{"speedUp":0.01,"speed":0.05},"nameColor":"#a9f548","say":"Wait, Seattle! Let me help you with that!","dir":1,"side":2,"action":[{"type":"shake","y":30,"delay":0,"dur":0.15,"number":2,"x":0}],"painting":{"time":1,"alpha":0.3},"bgName":"star_level_bg_100","actor":301231},{"expression":2,"nameColor":"#a9f548","say":"Hmm? Never mind, we're about done over here anyway. I will accompany Her Majesty to the party later, so do continue the good work, Seattle.","dir":1,"hidePaintObj":true,"side":2,"painting":{"time":1,"alpha":0.3},"typewriter":{"speedUp":0.01,"speed":0.05},"actor":299011,"bgName":"star_level_bg_100","blackBg":true}],"mode":2,"fadein":1.5,"id":"RUOYESHENGDAN2","fadeOut":1.5}