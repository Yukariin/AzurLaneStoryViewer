"flashin":{"black":true,"delay":1,"dur":1,"alpha":[1,0]},"flashout":{"black":true,"dur":1,"alpha":[0,1]}},{"side":2,"bgName":"bg_story_task","typewriter":{"speedUp":0.01,"speed":0.05},"nameColor":"#a9f548","say":"Commander, what are you looking at?","dir":1,"painting":{"time":1,"alpha":0.3},"actor":107030},{"side":2,"bgName":"bg_story_task","typewriter":{"speedUp":0.01,"speed":0.05},"say":"I raised my head to find Sara staring right at me, her face extremely close.","dir":1}]}