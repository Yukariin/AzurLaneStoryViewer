k":true,"dur":1},"bgName":"bg_story_task","dir":1,"flashout":{"alpha":[0,1],"black":true,"dur":1},"say":"こうして、この母港に新しい仲間が迎え入れられた――","nameColor":"#a9f548","side":2,"painting":{"time":1,"alpha":0.3},"typewriter":{"speed":0.05,"speedUp":0.01}},{"bgName":"bg_story_task","dir":1,"say":"ダイドーに任せる仕事はありますか？なんでもいいです！ダイドーはご主人様のためならなんでもします！","nameColor":"#a9f548","side":2,"expression":2,"painting":{"time":1,"alpha":0.3},"actor":202040,"typewriter":{"speed":0.05,"speedUp":0.01}},{"bgName":"bg_story_task","dir":1,"say":"だってダイドーは――ダイドーは、ご主人様のメイドですから！","nameColor":"#a9f548","painting":{"time":1,"alpha":0.3},"action":[{"y":30,"x":0,"dur":0.15,"delay":0,"number":2,"type":"shake"}],"side":2,"expression":2,"blackBg":true,"actor":202040,"typewriter":{"speed":0.05,"speedUp":0.01}}],"fadeOut":1.5,"fadeType":2,"fadein":1.5,"mode":2,"once":true}