:{"speed":0.05,"speedUp":0.01}},{"bgName":"star_level_bg_107","dir":-1,"say":"……ふん！そんなことない。普通に気をかけてやってるだけだ","nameColor":"#a9f548","side":0,"expression":2,"painting":{"time":1,"alpha":0.3},"actor":201011,"typewriter":{"speed":0.05,"speedUp":0.01}},{"bgName":"star_level_bg_107","dir":1,"actorName":"{namecode:2}","say":"（否定してるようで否定してないような……アマゾン、指揮官がいないと素直になるよね～）","nameColor":"#a9f548","side":1,"expression":1,"painting":{"time":1,"alpha":0.3},"actor":301013,"typewriter":{"speed":0.05,"speedUp":0.01}},{"bgName":"star_level_bg_107","dir":1,"actorName":"{namecode:2}","say":"みんなを集めて、この日ために練習したのもこのため？","nameColor":"#a9f548","side":1,"painting":{"time":1,"alpha":0.3},"actor":301013,"typewriter":{"speed":0.05,"speedUp":0.01}},{"bgName":"star_level_bg_107","dir":-1,"say":"違う！パーティーに必要だったからだぞ","nameColor":"#a9f548","side":0,"expression":1,"painting":{"time":1,"alpha":0.3},"actor":201011,"typewriter":{"speed":0.05,"speedUp":0.01}},{"painting":{"time":1,"alpha":0.3},"typewriter":{"speed":0.05,"speedUp":0.01},"bgName":"star_level_bg_107","side":0,"dir":-1,"say":"とにかく、今回のパーティーは、私たちのためのパーティーだ","actor":201011,"nameColor":"#a9f548"},{"painting":{"time":1,"alpha":0.3},"typewriter":{"speed":0.05,"speedUp":0.01},"bgName":"star_level_bg_107","side":0,"dir":-1,"say":"日頃頑張っているからこそ今日だけは胸を張って贅沢できる","actor":201011,"nameColor":"#a9f548"},{"painting":{"time":1,"alpha":0.3},"typewriter":{"speed":0.05,"speedUp":0.01},"bgName":"star_level_bg_107","side":0,"dir":-1,"say":"それと同時に忘れてはならない。私たちがこうしている間にも、頑張って仕事をしている人がいることを――","actor":201011,"nameColor":"#a9f548"},{"bgName":"star_level_bg_107","dir":1,"actorName":"{namecode:2}","say":"せめて今晩だけは指揮官をリラックスさせたい、ってことでいいよね？","nameColor":"#a9f548","side":1,"painting":{"time":1,"alpha":0.3},"actor":301013,"typewriter":{"speed":0.05,"speedUp":0.01}},{"painting":{"time":1,"alpha":0.3},"typewriter":{"speed":0.05,"speedUp":0.01},"bgName":"star_level_bg_107","side":0,"dir":-1,"say":"そうだ。はあ、あのバカ、まさか執務室で寝たんじゃないだろうな……","actor":201011,"nameColor":"#a9f548"},{"bgName":"star_