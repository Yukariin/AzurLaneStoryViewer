{"id":"CHANGDAO7","scripts":[{"mode":1,"sequence":[["幽霊さんはガンバラナイ\n\n<size=45>その七　さらば休暇よ<\/size>",1]]},{"painting":{"time":1,"alpha":0.3},"typewriter":{"speed":0.05,"speedUp":0.01},"side":2,"dir":1,"say":"も、戻った……うふふ……やっぱりここがロングアイランドさんにとっての一番……","actor":106011,"nameColor":"#a9f548"},{"painting":{"time":1,"alpha":0.3},"typewriter":{"speed":0.05,"speedUp":0.01},"side":2,"dir":1,"say":"我が人生に悔い無し……！ずっとこうしていたい……！","actor":106011,"nameColor":"#a9f548"},{"painting":{"time":1,"alpha":0.3},"typewriter":{"speed":0.05,"speedUp":0.01},"side":2,"dir":1,"say":"…誰？し、指揮官さん！？","actor":106011,"nameColor":"#a9f548"},{"painting":{"time":1,"alpha":0.3},"typewriter":{"speed":0.05,"speedUp":0.01},"side":2,"dir":1,"say":"言っとくけど！今日はもう出掛けないよ！","actor":106011,"nameColor":"#a9f548"},{"painting":{"time":1,"alpha":0.3},"typewriter":{"speed":0.05,"speedUp":0.01},"side":2,"dir":1,"say":"最後に一回？……終わらせたらゲームとプラモを買ってくれるの？","actor":106011,"nameColor":"#a9f548"},{"painting":{"time":1,"alpha":0.3},"typewriter":{"speed":0.05,"speedUp":0.01},"side":2,"dir":1,"say":"ううぅ……い、行くからぁ！せっかくの休暇なのに――","actor":106011,"nameColor":"#a9f548"}],"fadeOut":1.5,"fadeType":1,"fadein":1.5,"mode":2,"once":true}